<aside class="right_content">
    <div class="latest_post">
        <h2><span>Popular post</span></h2>
        <div class="latest_post_container">
            <div id="prev-button"><i class="fa fa-chevron-up"></i></div>
            <ul class="latest_postnav">
                <% mostViews.forEach(item => { %>
                    <li>
                        <div class="media"> <a href="/detail<%=item.Id%>" class="media-left"> <img alt="" src="images/<%= item.FileName %>" style="object-fit: cover;"> </a>
                            <div class="media-body"> <a href="/detail<%=item.Id%>" class="catg_title" style="font-size: 20px; font-weight: bolder;"> <%= item.title %></a> 
                                <p><%= item.view %> <i class="fa fa-user" style="color: rgb(173, 173, 173);"></i> - <%= moment(item.createAt).format("LLL") %></p>
                            </div>
                        </div>
                    </li>
                <% }) %>
            </ul>
            <div id="next-button"><i class="fa  fa-chevron-down"></i></div>
        </div>
    </div>
        <div class="single_sidebar">
            <ul class="nav nav-tabs" role="tablist">
                <li role="presentation" class="active"><a href="../user#category" aria-controls="home" role="tab" data-toggle="tab">Category</a></li>
                <li role="presentation"><a href="../user#video" aria-controls="profile" role="tab" data-toggle="tab">Video</a></li>
            </ul>
            <div class="tab-content">
                <div role="tabpanel" class="tab-pane active" id="category">
                    <ul>
                        <li class="cat-item"><a href="#sport">Sports</a></li>
                        <li class="cat-item"><a href="#weather">Weather</a></li>
                        <li class="cat-item"><a href="#food">Food</a></li>
                        <li class="cat-item"><a href="#music">Music</a></li>
                        <li class="cat-item"><a href="#movie">Movie</a></li>
                        <li class="cat-item"><a href="#actor">Actor</a></li>
                        <li class="cat-item"><a href="#photography">Photography</a></li>
                    </ul>
                </div>
                <div role="tabpanel" class="tab-pane" id="video">
                    <div class="vide_area">
                        <iframe width="100%" height="250" src="https://www.youtube.com/embed/mnQMvKnvEKs" frameborder="0" allowfullscreen></iframe>
                    </div>
                </div>
            </div>
        </div>
        <div class="single_sidebar wow fadeInDown">
            <h2><span>Sponsor</span></h2>
            <a class="sideAdd" href="../user#"><img src="images/add_img.jpg" alt=""></a> </div>
        <div class="single_sidebar wow fadeInDown">
            <h2><span>Category Archive</span></h2>
            <select class="catgArchive">
                <option>Select Category</option>
                <option>Life styles</option>
                <option>Sports</option>
                <option>Technology</option>
                <option>Treads</option>
            </select>
        </div>
        <div class="single_sidebar wow fadeInDown">
            <h2><span>Links</span></h2>
            <ul>
                <li><a href="#">Blog</a></li>
                <li><a href="#">Rss Feed</a></li>
                <li><a href="/login">Login</a></li>
                <li><a href="#">Life &amp; Style</a></li>
            </ul>
        </div>
</aside>