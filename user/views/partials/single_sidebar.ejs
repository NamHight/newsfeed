<aside class="right_content">
        <div class="single_sidebar">
            <h2><span>Popular Post</span></h2>
            <ul class="spost_nav">
                <% mostViews.forEach(item => { %>
                    <li>
                        <div class="media wow fadeInDown"> <a href="/news?id=<%=item.Id%>" class="media-left"> <img alt="" src="images/<%= item.FileName %>" style="object-fit: cover;"> </a>
                            <div class="media-body" width="100px"> <a href="/news?id=<%=item.Id%>" class="catg_title"> <h4><%= item.title %></h4></a> 
                                <p><%= item.view %> <i class="fa fa-user" style="color: rgb(173, 173, 173);"></i></p>
                            </div>
                        </div>
                    </li>
                <% })  %>
               
            </ul>
        </div>
        <div class="single_sidebar">
            <ul class="nav nav-tabs" role="tablist">
                <li role="presentation" class="active"><a href="../user#category" aria-controls="home" role="tab" data-toggle="tab">Category</a></li>
                <li role="presentation"><a href="../user#video" aria-controls="profile" role="tab" data-toggle="tab">Video</a></li>
                <li role="presentation"><a href="../user#comments" aria-controls="messages" role="tab" data-toggle="tab">Comments</a></li>
            </ul>
            <div class="tab-content">
                <div role="tabpanel" class="tab-pane active" id="category">
                    <ul>
                        <li class="cat-item"><a href="#">Sports</a></li>
                        <li class="cat-item"><a href="#">Fashion</a></li>
                        <li class="cat-item"><a href="#">Business</a></li>
                        <li class="cat-item"><a href="#">Technology</a></li>
                        <li class="cat-item"><a href="#">Games</a></li>
                        <li class="cat-item"><a href="#">Life &amp; Style</a></li>
                        <li class="cat-item"><a href="#">Photography</a></li>
                    </ul>
                </div>
                <div role="tabpanel" class="tab-pane" id="video">
                    <div class="vide_area">
                        <iframe width="100%" height="250" src="https://www.youtube.com/embed/mnQMvKnvEKs" frameborder="0" allowfullscreen></iframe>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane" id="comments">
                    <ul class="spost_nav">
                        <% comments.forEach(item => {%>
                        <li>
                            <div class="media wow fadeInDown"><img alt="" src="images/Anh.png" class="media-left">
                                <div class="media-body" style="display: block;"><%= item.Content %></div>
                            </div>
                        </li>
                        <% }) %>
                    </ul>
                </div>
            </div>
        </div>
        <div class="single_sidebar wow fadeInDown">
            <h2><span>Sponsor</span></h2>
            <a class="sideAdd" href="../user#"><img src="images/add_img.jpg" alt=""></a> </div>
        <div class="single_sidebar wow fadeInDown">
            <h2><span>Category Archive</span></h2>
            <select class="catgArchive">
                <option>Select Category</option>
                <option>Life styles</option>
                <option>Sports</option>
                <option>Technology</option>
                <option>Treads</option>
            </select>
        </div>
        <div class="single_sidebar wow fadeInDown">
            <h2><span>Links</span></h2>
            <ul>
                <li><a href="#">Blog</a></li>
                <li><a href="#">Rss Feed</a></li>
                <li><a href="/login">Login</a></li>
                <li><a href="#">Life &amp; Style</a></li>
            </ul>
        </div>
</aside>