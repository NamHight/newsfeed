

<div class="mt-5 container">
    <form action="update" method="post">
        <input type="hidden" class="form-control" id="id" name="id" value="<%=id%>" >
        <div class="form-group">
          <label for="Title">Title</label>
          <input type="text" class="form-control" id="Title" name="title" value="<%=data.title%>">
        </div>
        <div class="form-group">
          <label for="Description">Description</label>
          <input type="text" class="form-control" id="Description" name="description" value="<%=data.description%>">
        </div>
        <div class="form-group">
            <label for="Author">Author</label>
            <input type="text" class="form-control" id="Author" name="author" value="<%=data.author%>">
        </div>
        <div class="form-group">
            <label for="View">View</label>
            <input type="text" class="form-control" id="View" name="view" value="<%=data.view%>">
        </div>
        <div class="form-group">
            <label for="Catetory">Catetory</label>
            <input type="text" class="form-control" id="catetory" name="catetory" value="<%=data.catetory%>">
        </div>
        <div class="form-group">
            <label for="Status">Status</label>
            <input type="text" class="form-control" id="status" name="status" value="<%=data.status%>">
        </div>
        <button type="submit" class="btn btn-primary">Update</button>
    </form>
</div>

