

<div class="text-center rounded text-info h5" style="margin-bottom: 20px;"> 
    <marquee behavior="scroll" direction="left">
        <%=title %>
    </marquee>
</div>
<div class="container d-flex justify-content-between">
    <div>
        <a href="/newsletter" class="btn btn-warning active" aria-current="page">Home NewsLetter</a>
    </div>
</div>

<div class="mt-3 container">
    <table class="mt-2 table table-striped table-bordered table-sm text-center">
        <thead class="thead-info">
            <tr>
                <th>Id</th>
                <th>Mail</th>
                <th>Role</th>
                <th>CreateAt</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(element => { %>
                <tr>
                    <td><%=element.Id%></td>
                    <td><%=element.Mail%></td>
                    <td><%=element.Role%></td>
                    <td><%=date(element.CreateAt)%></td>
                </tr>
            <% });  %>
        </tbody>
    </table>
</div>
